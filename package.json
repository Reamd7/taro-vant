{
  "name": "taro-vant",
  "version": "1.0.2",
  "private": false,
  "description": "基于taro2进行多端组件开发，迁移vant-weapp的组件库，兼容h5、微信小程序端、支付宝小程序端。",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "less"
  },
  "scripts": {
    "build:component2": "cross-env TARO_BUILD_TYPE=component taro build --ui",
    "build:component": "node ./config/ui.js && yarn tsc -p ./src/components/taro-vant",
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Reamd7/taro-vant.git"
  },
  "author": "reamd7",
  "license": "MIT",
  "dependencies": {
    "mini-types": "^0.1.4",
    "miniprogram-api-typings": "^3.2.0",
    "@types/classnames": "^2.2.10",
    "@types/big.js": "^4.0.5",
    "big.js": "^6.0.1",
    "classnames": "^2.2.6",
    "dayjs": "^1.9.1",
    "resize-observer-polyfill": "^1.5.1",
    "throttle-debounce": "^3.0.1"
  },
  "devDependencies": {
    "glob": "^7.1.6",
    "nerv-devtools": "^1.5.5",
    "nervjs": "^1.5.7",
    "react": "16.8.0",
    "react-native": "0.59.9",
    "regenerator-runtime": "0.11.1",
    "tslib": "^1.8.0",
    "@tarojs/cli": "2",
    "@tarojs/components": "2.2.16",
    "@tarojs/components-qa": "2.2.16",
    "@tarojs/components-rn": "^2.2.16",
    "@tarojs/rn-runner": "^2.2.16",
    "@tarojs/router": "2.2.16",
    "@tarojs/service": "2",
    "@tarojs/taro": "2.2.16",
    "@tarojs/taro-alipay": "2.2.16",
    "@tarojs/taro-h5": "2.2.16",
    "@tarojs/taro-qq": "2.2.16",
    "@tarojs/taro-quickapp": "2.2.16",
    "@tarojs/taro-redux-rn": "^2.2.16",
    "@tarojs/taro-rn": "^2.2.16",
    "@tarojs/taro-router-rn": "^2.2.16",
    "@tarojs/taro-swan": "2.2.16",
    "@tarojs/taro-tt": "2.2.16",
    "@tarojs/taro-weapp": "2.2.16",
    "babel-runtime": "^6.26.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@tarojs/mini-runner": "2.2.16",
    "@tarojs/plugin-less": "2.2.16",
    "@tarojs/plugin-terser": "2.2.16",
    "@tarojs/webpack-runner": "2.2.16",
    "@types/react": "^16.4.6",
    "@types/throttle-debounce": "^2.1.0",
    "@types/webpack-env": "^1.13.6",
    "@types/wechat-miniprogram": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-jsx-stylesheet": "^0.6.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.9.0",
    "eslint-config-taro": "2.2.16",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "eslint-plugin-taro": "2.2.16",
    "eslint-plugin-typescript": "^0.14.0",
    "stylelint": "9.3.0",
    "stylelint-config-taro-rn": "2.2.16",
    "stylelint-taro-rn": "2.2.16",
    "typescript": "^3.9.7"
  }
}
