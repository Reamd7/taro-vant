{
  "name": "taro-vant",
  "version": "1.0.7",
  "private": false,
  "description": "基于taro2进行多端组件开发，迁移vant-weapp的组件库，兼容h5、微信小程序端、支付宝小程序端。",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "less"
  },
  "scripts": {
    "build:component": "node ./config/ui.js",
    "build:weapp": "yarn taro build --type weapp",
    "build:swan": "yarn taro build --type swan",
    "build:alipay": "yarn taro build --type alipay",
    "build:tt": "yarn taro build --type tt",
    "build:h5": "yarn taro build --type h5",
    "build:rn": "yarn taro build --type rn",
    "build:qq": "yarn taro build --type qq",
    "build:jd": "yarn taro build --type jd",
    "build:quickapp": "yarn taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch"
  },
